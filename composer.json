{
    "name": "laravel-open-admin-ext/ckeditor",
    "description": "Integrate CKEDITOR into open-admin [CUSTOM]",
    "type": "library",
    "keywords": [
        "open-admin",
        "wysiwyg",
        "extension",
        "editor",
        "CKEditor"
    ],
    "homepage": "https://github.com/open-admin-ext/ckeditor",
    "license": "MIT",
    "authors": [
        {
            "name": "open-admin",
            "email": "info@open-admin.org"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": "12.1.1.0",
        "dedermus/open-admin-core": "^2.0",
        "ckeditor/ckeditor": "4.25.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "12.*",
        "laravel/laravel": "12.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAdmin\\Admin\\CKEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdmin\\Admin\\CKEditor\\CKEditorServiceProvider"
            ]
        }
    }
}